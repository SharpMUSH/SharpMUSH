{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "ZiggyCreatures.Caching.Fusion": "Error",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "ConnectionServer": {
    "TelnetPort": 4201,
    "HttpPort": 4202,
    "TelnetDescriptorStart": 0,
    "WebSocketDescriptorStart": 1000000
  }
}
